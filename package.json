    "build-storybook": "build-storybook -s public",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions ts tsx --language typescript && relay-enum-generator",
    "get-schema": "graphql get-schema -e default"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
