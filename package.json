    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint --max-warnings=0 src/**/*.{ts,tsx}",
    "sort": "import-sort --write 'src/**/*.{ts,tsx,js,jsx}'",
    "sort:check": "import-sort -l 'src/**/*.{ts,tsx,js,jsx}'",
